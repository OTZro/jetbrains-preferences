<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="AngularJS" injector-id="xml">
      <display-name>ul/@[pk]</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().matches("[pk]")).withParent(xmlTag().withLocalName(string().equalTo("ul")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
  </component>
</application>